@model List<OurTeam>
<section class="w3l-team" id="team">
    <div class="team-block py-5">
        <div class="container py-lg-5">
            <div class="title-content text-center">
                <h6 class="sub-title">Şeflerimiz</h6>
                <h3 class="title-big">Lezzetin arkasındaki</h3>
            </div>
            <div class="row">
                @foreach (var item in Model)
                {

                    <div class="col-lg-3 col-sm-6 mt-lg-5 mt-4">
                        <div class="box16">
                            <a href="#url"><img src="~/pizzapan-starter/assets/images/team1.jpg" alt="" class="img-fluid radius-image" /></a>
                            <div class="box-content">
                                <h3 class="title"><a href="#url">@item.Name</a></h3>
                                <span class="post">Master chef</span>
                                <ul class="social">
                                    <li>
                                        <a href="#" class="facebook">
                                            <span class="fa fa-facebook-f"></span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="twitter">
                                            <span class="fa fa-twitter"></span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
</section>
