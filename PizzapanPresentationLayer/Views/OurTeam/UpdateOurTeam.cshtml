@model PizzapanEntityLayer.Concrete.OurTeam
@{
    ViewData["Title"] = "UpdateOurTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Takım Güncelleme Sayfası</h1>

<form method="post">
    <input type="hidden" asp-for="OurTeamId" />
    <label>İsim Soyisim </label>
    <input asp-for="Name" class="form-control" style="background-color:#b1b3b1" />
    <br />
    <label>Soyisim</label>
    <input asp-for="Surname" class="form-control" style="background-color:#b1b3b1" />
    <br />
    <label>Başlık</label>
    <input asp-for="Title" class="form-control" style="background-color:#b1b3b1" />
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>